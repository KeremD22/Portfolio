
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Deneyim Ekleme Sayfası</h2>

<form method="post">
    <label>Deneyim</label>
    <input type="text" class="form-control" name="ExperienceName" />
    <br />
    <label>Deneyim Ünvanı</label>
    <input type="text" class="form-control" name="ExperienceTestimonial" />
    <br />
    <label>Deneyim Yılı</label>
    <input type="text" class="form-control" name="ExperienceYear" />
    <br />
    <button class="btn btn-info">Deneyim Ekle</button>

</form>
