@model List<Portfolio.Models.About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}





<h2>Tecrübe Sayfası</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Okul Adı</th>
        <th>Eğitim Süresi</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>@item.Introduction</td>
            <td>@item.Description</td>n

            <td><a href="/About/DeleteAbout/@item.AboutID" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/About/UpdateAbout/@item.AboutID" class="btn btn-outline-success">Güncelle</a></td>
        </tr>
    }
</table>
